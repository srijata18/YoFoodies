<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item >
        <shape
            android:shape="oval">
            <solid android:color="#ddd" />
            <corners android:radius="@dimen/dimen_50dp"/>
            <padding android:right="@dimen/dimen_2dp" android:bottom="@dimen/dimen_2dp"/>
        </shape>
    </item>
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval" >
            <corners
                android:radius="@dimen/dimen_50dp" />
            <padding android:bottom="@dimen/dimen_8dp"
                android:left="@dimen/dimen_8dp"
                android:right="@dimen/dimen_8dp"
                android:top="@dimen/dimen_8dp"/>
            <solid android:color="@android:color/white"/>
            <stroke
                android:width="1dp"
                android:color="@android:color/darker_gray" />
        </shape>
    </item>


    <item android:drawable="@drawable/ic_add_a_photo"/>
</layer-list>